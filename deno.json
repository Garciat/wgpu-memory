{
  "name": "@garciat/wgpu-memory",
  "version": "0.1.0",
  "exports": {
    ".": "./src/wgpu-memory.ts"
  },
  "publish": {
    "exclude": [
      ".devcontainer",
      ".github",
      ".vscode",
      "examples",
      "docs",
      "**/*.test.ts"
